FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0.5";
    object      rDeltaT;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 -1 0 0 0 0];

internalField   uniform 100000;

boundaryField
{
    outlet_LHS
    {
        type            extrapolatedCalculated;
        value           uniform 100000;
    }
    outlet_RHS
    {
        type            extrapolatedCalculated;
        value           uniform 100000;
    }
    secondary_air_LHS
    {
        type            extrapolatedCalculated;
        value           uniform 100000;
    }
    secondary_air_RHS
    {
        type            extrapolatedCalculated;
        value           uniform 100000;
    }
    top_fuel
    {
        type            extrapolatedCalculated;
        value           uniform 100000;
    }
    L_fuel
    {
        type            extrapolatedCalculated;
        value           uniform 100000;
    }
    R_fuel
    {
        type            extrapolatedCalculated;
        value           uniform 100000;
    }
    bottom_fuel
    {
        type            extrapolatedCalculated;
        value           uniform 100000;
    }
    primary_inlets
    {
        type            extrapolatedCalculated;
        value           uniform 100000;
    }
    stove_body
    {
        type            extrapolatedCalculated;
        value           uniform 100000;
    }
    frontAndBack
    {
        type            empty;
    }
    defaultFaces
    {
        type            empty;
    }
}


