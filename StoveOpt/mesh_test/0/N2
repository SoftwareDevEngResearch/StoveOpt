FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      N2;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0.77;

boundaryField
{
    top_fuel
    {
        type            fixedValue;
        value           uniform 0.0;
    }
	L_fuel
    {
        type            fixedValue;
        value           uniform 0.0;
    }
	R_fuel
    {
        type            fixedValue;
        value           uniform 0.0;
    }
	bottom_fuel
    {
        type            fixedValue;
        value           uniform 0.0;
    }
    primary_inlets
    {
        type            fixedValue;
        value           uniform 0.77;
    }
	secondary_air_LHS
	{
		type			fixedValue;
		value			uniform 0.77;
	}	
	secondary_air_RHS
	{
		type			fixedValue;
		value			uniform 0.77;
	}		
	stove_body
	{
		type			fixedValue;
		value			uniform 0.77;
	}
    outlet_LHS
    {
        type            inletOutlet;
        inletValue      uniform 0.77;
        value           uniform 0.77;
    }
	outlet_RHS
    {
        type            inletOutlet;
        inletValue      uniform 0.77;
        value           uniform 0.77;
    }
    frontAndBack
    {
        type            empty;
    }
}

